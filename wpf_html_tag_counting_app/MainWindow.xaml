<Window x:Class="wpf_html_tag_counting_app.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpf_html_tag_counting_app"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,78,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" MaxHeight="500" Width="411" Height="346" />
        <TextBlock x:Name="highestValueTextblock" HorizontalAlignment="Left" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" RenderTransformOrigin="4.145,-15.586" Height="32" Width="365" Margin="0,10,0,0"/>
        <Button x:Name="button_open" Content="Open file" HorizontalAlignment="Left" VerticalAlignment="Top" Height="54" Width="204" FontSize="30" Click="ButtonOpenClick" Margin="432,241,0,0"/>
        <Button x:Name="button_cancel" Content="Cancel" HorizontalAlignment="Left" Margin="432,323,0,0" VerticalAlignment="Top" Height="50" Width="204" FontSize="30" Click="ButtonCancelClick"/>
        <ProgressBar x:Name="myProgressBar" HorizontalAlignment="Left" Height="40" Margin="432,177,0,0" VerticalAlignment="Top" Width="204"/>
    </Grid>
</Window>
